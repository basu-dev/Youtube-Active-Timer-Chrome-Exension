{
    "manifest_version":2,
    "name":"YoutubeTimer",
    "description":"YOutube Timer Extension",
    "version":"1.0.0",
    "browser_action":{
        
        "default_popup":"popup.html"
        
    },
    "content_scripts":[
        {
            "matches":["https://www.youtube.com/"],
            "js":["content.js"]
        }
    ],
  
    "permissions":["tabs","storage"]    
}